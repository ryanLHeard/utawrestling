<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <% include templates/head.ejs %>
</head>
<body>
    <div class="container">
        <% include templates/header.ejs %>
        <div  class="colDecor">
            <legend><h3>Active Wrestlers</h3></legend>
            <legend><h3>Former Wrestlers</h3></legend>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Name</th>
                    <th>Position</th>
                    <th>Weight</th>
                    <th>Major</th>

                </tr>
                <% people.forEach(function(person){%>
                    <tr>
                        <td><%= person.firstName + " "+person.lastName %></td>
                        <td><%= person.role %></td>
                        <td><%= person.highSchool %></td>
                        <td><%= person.major %></td>
                    </tr>
                <%}); %>
            </table>

        </div>
    </div>
</body>
</html>
